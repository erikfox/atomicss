  },
  "bugs": {
    "url": "https://ghe.spotify.net/node/atomicss/issues"
  },
  "keywords": [],
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm run build -- --watch & npm test -- --watchAll & wait",
